

public class Camel implements Rideable{
	
	int weight=2;
	public static void main(String [] args){
		Camel cam=new Camel();
	cam.go(8);
	}	
	void go(int speed){
		++speed;
		weight++;
		int walkrate=speed*weight;
		System.out.println("walkRate:  "+getGaint());
	}
	public String getGaint(){
		return "mph, lope";
	}
}
public interface Rideable
{
	String getGaint ();	
}
